{
    split_year <- split_on_decimal(as.character(year))
    split_year <- sapply(split_year, function(x) {
        if (length(x) == 1) 
            return(0)
        else return(as.numeric(paste0("0.", x[2])))
    })
    return(split_year)
  }
}
